<div class="power-selector-with-label">
    <span class="power-selector-with-label-label"> {{placeholder}} </span>
    <div class="power-selector-with-label-selector">
        {{#power-select selected=selected placeholder=placeholder options=options onchange=onchange as |option| }}
            {{yield option}}
        {{/power-select}}
    </div>
</div>